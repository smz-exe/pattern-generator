# Environment Variables for Pattern Generator
# Copy this file to .env.local for local development

# Vercel Environment Variables
# These are automatically provided by Vercel
# VERCEL_ENV=development|preview|production
# VERCEL_URL=your-deployment-url.vercel.app
# VERCEL_REGION=iad1

# Application Configuration
# VITE_APP_TITLE=Pattern Studio
# VITE_APP_DESCRIPTION=Interactive 3D Generative Art Laboratory

# Performance Configuration
# VITE_PERFORMANCE_MONITOR=true
# VITE_DEBUG_MODE=false

# Analytics (if using Vercel Analytics)
# VITE_ANALYTICS_ID=your-analytics-id

# Custom Domain (if configured)
# VITE_CUSTOM_DOMAIN=your-domain.com

# Feature Flags
# VITE_ENABLE_EXPERIMENTAL_FEATURES=false
# VITE_ENABLE_WEB_WORKERS=false
# VITE_ENABLE_WEBGL=false